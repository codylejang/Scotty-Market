{
  "name": "scottymarket-backend",
  "version": "1.0.0",
  "description": "Agentic AI backend for Scotty gamified financial management",
  "main": "dist/index.js",
  "scripts": {
    "dev": "node --env-file=.env node_modules/.bin/tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "migrate": "tsx src/db/migrate.ts",
    "seed": "tsx src/db/seed.ts",
